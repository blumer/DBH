{
  "name": "Dan Benjamin Hour",
  "short_name": "DBH",
  "description": "Mini-Viewer for the Dan Benjamin Hour",
  "version": "0.3",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "dbh-16.png", "128": "dbh-128.png" },
  "permissions": [
	"webview",
	"http://*.metacdn.com/",
	"https://*.kiwiirc.com/"
  ],
  "commands": {
	"toggle-orientation": {
		"suggested_key": {
			"default": "Ctrl+Shift+Y",
			"mac": "Command+Shift+Y"
		},
		"description": "Toggle Orientation"
	}
	}
}

